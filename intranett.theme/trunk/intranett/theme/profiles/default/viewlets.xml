<?xml version="1.0"?>
<object>
    <order manager="intranett.topbar" skinname="Intranett.no base theme">
      <viewlet name="plone.personal_bar" />
    </order>    
    <order manager="plone.portalheader" skinname="Intranett.no base theme" based-on="Plone Default">
      <viewlet name="plone.logo" insert-after="plone.skip_links" />
    </order>
    <order manager="plone.belowcontentbody" skinname="Intranett.no base theme" based-on="Plone Default">
      <viewlet name="plone.belowcontenttitle.documentbyline" insert-after="*" />
    </order>
    <order manager="plone.portalfooter" skinname="Intranett.no base theme" based-on="Plone Default">
      <viewlet name="plone.site_actions" insert-before="*" />
    </order>
    <order manager="intranett.abovecolumns" skinname="Intranett.no base theme">
      <viewlet name="plone.path_bar" />
    </order>    

    <hidden manager="plone.portalheader" skinname="Intranett.no base theme">
      <viewlet name="plone.personal_bar" />
    </hidden>       
    <hidden manager="plone.abovecontent" skinname="Intranett.no base theme">
      <viewlet name="plone.path_bar" />
    </hidden>
    <hidden manager="plone.belowcontenttitle" skinname="Intranett.no base theme">
      <viewlet name="plone.belowcontenttitle.documentbyline" />
    </hidden>
</object>
